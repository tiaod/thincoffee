{
  "name": "thincoffee",
  "version": "0.0.0",
  "dependencies": {
    "jquery": "~2.1.4",
    "moment": "~2.10.6",
    "page": "~1.6.4",
    "formstone": "~0.8.27",
    "bootstrap-multiselect": "~0.9.13",
    "eonasdan-bootstrap-datetimepicker": "~4.17.37",
    "image-picker": "~0.2.4",
    "masonry": "~3.3.2",
    "parsleyjs": "~2.1.3"
  },
  "devDependencies": {
    "angular-mocks": "~1.4.2"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/fonts/glyphicons-halflings-regular.eot",
        "dist/fonts/glyphicons-halflings-regular.svg",
        "dist/fonts/glyphicons-halflings-regular.ttf",
        "dist/fonts/glyphicons-halflings-regular.woff",
        "dist/fonts/glyphicons-halflings-regular.woff2"
      ]
    },
    "formstone": {
      "main": [
        "dist/js/core.js",
        "dist/**/*"
      ]
    },
    "moment": {
      "main": [
        "moment.js",
        "locale/zh-cn.js"
      ]
    },
    "modernizr": {
      "main": "./modernizr.js"
    },
    "masonry": {
      "main": [
        "./dist/masonry.pkgd.js"
      ],
      "dependencies": {}
    },
    "parsleyjs": {
      "main": [
        "dist/parsley.js",
        "src/i18n/zh_cn.js",
        "src/parsley.css"
      ]
    },
    "image-picker":{
      "main": ["image-picker/image-picker.js","image-picker/image-picker.css"]
    }
  },
  "resolutions": {
    "jquery": "~2.1.4"
  }
}
